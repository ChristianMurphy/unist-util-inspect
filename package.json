{
  "name": "unist-util-inspect",
  "version": "4.0.0",
  "description": "Unist node inspector",
  "license": "MIT",
  "keywords": [
    "retext",
    "remark",
    "unist",
    "inspect"
  ],
  "dependencies": {
    "is-empty": "1.0.0"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/unist-util-inspect.git"
  },
  "bugs": "https://github.com/wooorm/unist-util-inspect/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "chalk": "^1.0.0",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.1",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^4.0.0",
    "remark-comment-config": "^3.0.0",
    "remark-github": "^4.0.1",
    "remark-lint": "^3.0.0",
    "remark-slug": "^4.0.0",
    "remark-validate-links": "^3.0.0",
    "retext": "^2.0.0",
    "tape": "^4.4.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s unistUtilInspect > unist-util-inspect.js",
    "build-mangle": "esmangle unist-util-inspect.js > unist-util-inspect.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
